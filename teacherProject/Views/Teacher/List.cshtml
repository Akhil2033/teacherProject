
@model IEnumerable<teacherProject.Models.Teacher>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>This page shows information about all teachers</p>
<form action="/Teacher/List" method="GET">
    <input type="text" name="SearchKey" />
    <input type="submit" value="Search" />
</form>

@foreach(var Teacher in Model) { 

    <div>
    <a href="/Teacher/Show/@Teacher.TeacherId">@Teacher.TeacherFname @Teacher.TeacherLname</a>
    </div>

 
}

